---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xml:base="http://posulliv.github.com">
 <channel>

  <title>Padraig O'Sullivan - Drupal</title>
  <link>http://posulliv.gitub.com</link>
  <description/>
  <language>en</language>

  {% for post in site.posts %}
    {% for cat in post.categories %}
      {% if post.category == 'planet drupal' %}
        <item>
          <title>{{ post.title }}</title>
          <link>http://posulliv.github.com/{{ post.url }}</link>
          <description>
            {{ post.content | xml_escape }}
          </description>
          <pubDate>{{ post.date }}</pubDate>
        </item>
      {% endif %}
    {% endfor %}
  {% endfor %}

 </channel>
</rss>
